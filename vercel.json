{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(tool|about)", "dest": "/api/index.js" },
      { "src": "/", "dest": "/api/index.js" },
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "handle": "filesystem" }
    ]
  }